{
    "name": "garbuzivan/imagemanager",
    "description": "Manager image library for Laravel",
    "type": "library",
    "require": {
        "spatie/laravel-image-optimizer": "^1.6",
        "intervention/image": "^2.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": ">=1.3"
    },
    "authors": [
        {
            "name": "Garbuz Ivan",
            "email": "garbuzivan90@ya.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GarbuzIvan\\ImageManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GarbuzIvan\\ImageManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GarbuzIvan\\ImageManager\\ImageManagerServiceProvider"
            ]
        }
    }
}
